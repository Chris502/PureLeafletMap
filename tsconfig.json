{
    "compilerOptions": {
      "module": "es2015",
      "target": "es5",
      "outDir": "./dist",
      "lib": ["es6", "dom"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "rootDir": "src",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true
    },
  "include": [
    "./src/*"
  ],
    "exclude": [
      "node_modules"
    ]
  }
